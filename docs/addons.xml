<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="repository.chainlabswizard" version="1.0.1" name="ChainLabs Wizard Repository" provider-name="YourName">
  <extension point="xbmc.addon.repository" name="ChainLabs Wizard Repo">
    <info compressed="false">addons.xml</info>
  </extension>
  <summary lang="en">Hosts the ChainLabs Wizard program add-on</summary>
  <description lang="en">
    Contains the program add-on that installs the full Neurochain Kodi build.
  </description>
  <platform>all</platform>
</addon><addon id="plugin.program.chainlabswizard" name="ChainLabs Wizard" version="1.0.1" provider-name="YourName">
